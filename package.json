{
  "name": "app-name-change-me",
  "version": "0.0.1",
  "description": "App description (Change me)",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build:production": "NODE_ENV=production gulp build",
    "build": "gulp build",
    "postbuild": "cordova platform rm browser && cordova platform add browser && phonegap serve"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-css": "^0.9.2",
    "del": "^2.2.2",
    "gulp-appfy-tasks": "^3.5.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread",
            "add-module-exports",
            "transform-object-assign"
          ]
        }
      ],
      [
        "browserify-css",
        {
          "autoInject": true,
          "minify": true
        }
      ]
    ]
  },
  "keywords": [
    "phonegap",
    "es6",
    "template"
  ],
  "dependencies": {
    "cordova-browser": "^4.1.0",
    "cordova-ios": "^4.4.0",
    "cordova-plugin-network-information": "^1.2.1",
    "cordova-plugin-whitelist": "^1.2.2",
    "hyperscript": "^2.0.2",
    "hyperscript-helpers": "^3.0.3",
    "jquery": "^3.2.1",
    "localforage": "^1.5.0",
    "whatwg-fetch": "^1.0.0"
  },
  "author": "Sacha Spinelli",
  "license": "MIT",
  "cordova": {
    "plugins": {
      "cordova-plugin-network-information": {},
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "ios",
      "android"
    ]
  }
}
