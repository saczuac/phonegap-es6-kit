{
  "name": "app-name-change-me",
  "version": "0.0.1",
  "description": "App description (Change me)",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "build-serve": "gulp build && phonegap serve",
    "build-android": "gulp build && cordova platform rm android && cordova platform add android@5.1.0",
    "inject:loader": "node scripts/inject.js",
    "isolate:css": "node scripts/isolate-css.js",
    "build:production": "NODE_ENV=production npm-run-all build inject:loader isolate:css"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-css": "^0.9.2",
    "del": "^2.2.2",
    "gulp-appfy-tasks": "^3.5.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-parent-selector": "^1.0.0",
    "rimraf": "^2.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread",
            "add-module-exports",
            "transform-object-assign"
          ]
        }
      ],
      [
        "browserify-css",
        {
          "autoInject": true,
          "minify": true
        }
      ]
    ]
  },
  "keywords": [
    "phonegap",
    "es6",
    "template"
  ],
  "dependencies": {
    "cordova-android": "^5.1.1",
    "cordova-browser": "^4.1.0",
    "classnames": "^2.2.5",
    "cordova-plugin-compat": "^1.1.0",
    "cordova-plugin-dialogs": "^1.3.3",
    "cordova-plugin-geolocation": "^2.4.3",
    "cordova-plugin-network-information": "^1.3.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "hyperscript": "^2.0.2",
    "jquery": "^3.2.1",
    "localforage": "^1.4.3",
    "whatwg-fetch": "^1.0.0"
  },
  "author": "Sacha Spinelli",
  "license": "MIT",
  "cordova": {
    "plugins": {
      "cordova-plugin-dialogs": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  }
}
